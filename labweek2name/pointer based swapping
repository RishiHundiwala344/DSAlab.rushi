#include <stdio.h>

void swap(int *p,int *q){
    int temp;
    temp=*p;
    *p=*q;
    *q=temp;
    printf("the swapped numbers are: %d\t%d\n", *p,*q);
}

void main(){
    int a,b;
    printf("enter two numbers: \n");
    scanf("%d%d",&a,&b);
    swap(&a,&b);
}

//output
//enter two numbers
//2 3
//the swapped numbers are 3 2
